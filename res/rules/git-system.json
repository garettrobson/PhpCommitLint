{
    "ruleSets" : {
        "git-system": {
            "git-merge-message": {
                "type": "property-regex",
                "property": "summary",
                "pattern": "/^Merge \\w+ '[^']+'( into '([^']*)'|$)/",
                "positiveCheck": false,
                "errorMessage": "Merge branch",
                "pass": true
            }
        }
    }
}
